    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "daisyui": "^4.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "i18next": "^23.7.20",
    "i18next-browser-languagedetector": "^7.2.0",
